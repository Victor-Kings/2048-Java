<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.tensorflow.lite.examples.detection">

    <uses-sdk />

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:networkSecurityConfig="@xml/netowrk_security_config"
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hello"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaVoz"></activity>
        <activity android:name=".TelaTensorFlow" />
        <activity android:name=".TelaSeta" />

        <activity android:name=".MainActivity">

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity android:name=".DetectorActivity"
                  android:label="@string/activity_name_detection"
                  android:screenOrientation="portrait">
        </activity>

    </application>
</manifest>
